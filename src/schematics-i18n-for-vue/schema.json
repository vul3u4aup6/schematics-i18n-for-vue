{
    "$schema": "http://json-schema.org/schema",
    "title": "quick-i18n",
    "type": "object",
    "description": "quick generate i18n",
    "properties": {
        "key": {
            "type": "string",
            "$default": {
                "$source": "argv",
                "index": 0
            },
            "x-prompt": "Enter i18n Set (ex: Common.Tip or Tip)"
        },
        "tw": {
            "type": "string",
            "$default": {
                "$source": "argv",
                "index": 1
            },
            "x-prompt": "Enter zh-TW value"
        },
        "zh": {
            "type": "string",
            "$default": {
                "$source": "argv",
                "index": 1
            },
            "x-prompt": "Enter zh-CN value"
        },
        "en": {
            "type": "string",
            "$default": {
                "$source": "argv",
                "index": 1
            },
            "x-prompt": "Enter zh-EN value"
        },
        "vi": {
            "type": "string",
            "$default": {
                "$source": "argv",
                "index": 1
            },
            "x-prompt": "Enter zh-VI value"
        }
    },
    "required": [
        "key",
        "tw",
        "zh",
        "en",
        "vi"
    ]
}
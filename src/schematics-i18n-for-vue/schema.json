{
    "$schema": "http://json-schema.org/schema",
    "title": "quick-i18n",
    "type": "object",
    "description": "quick generate i18n",
    "properties": {
        "key": {
            "type": "string",
            "$default": {
                "$source": "argv",
                "index": 0
            },
            "x-prompt": "Enter i18n Set (ex: Common.Tip or Tip)"
        },
        "cn": {
            "type": "string",
            "$default": {
                "$source": "argv",
                "index": 1
            },
            "x-prompt": "Enter zh-CN value"
        },
        "en": {
            "type": "string",
            "$default": {
                "$source": "argv",
                "index": 1
            },
            "x-prompt": "Enter zh-EN value"
        },
        "jp": {
            "type": "string",
            "$default": {
                "$source": "argv",
                "index": 1
            },
            "x-prompt": "Enter zh-JP value"
        },
        "vn": {
            "type": "string",
            "$default": {
                "$source": "argv",
                "index": 1
            },
            "x-prompt": "Enter zh-VN value"
        }
    },
    "required": [
        "key",
        "cn",
        "en",
        "jp",
        "vn"
    ]
}